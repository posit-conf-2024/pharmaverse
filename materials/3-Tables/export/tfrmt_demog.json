{
  "group": ["variable"],
  "label": ["label"],
  "param": ["stat_name"],
  "value": ["stat"],
  "column": ["TRT01A"],
  "row_grp_plan": {
    "struct_list": [
      {
        "group_val": [".default"],
        "block_to_apply": {
          "post_space": ["  "],
          "fill": [true]
        }
      }
    ],
    "label_loc": {
      "location": ["indented"],
      "indent": ["  "]
    }
  },
  "body_plan": [
    {
      "group_val": [".default"],
      "label_val": [".default"],
      "param_val": ["n", "p"],
      "frmt_combine": {
        "expression": ["{n} ({p}%)"],
        "frmt_ls": {
          "n": {
            "frmt": {
              "expression": ["xx"],
              "missing": {},
              "scientific": {},
              "transform": {}
            }
          },
          "p": {
            "frmt_when": {
              "frmt_ls": {
                "<0.01": ["<1"],
                "TRUE": {
                  "frmt": {
                    "expression": ["xx"],
                    "missing": {},
                    "scientific": {},
                    "transform": ["~. * 100"]
                  }
                }
              },
              "missing": {}
            }
          }
        },
        "missing": {}
      }
    },
    {
      "group_val": [".default"],
      "label_val": [".default"],
      "param_val": ["mean", "sd"],
      "frmt_combine": {
        "expression": ["{mean} ({sd})"],
        "frmt_ls": {
          "mean": {
            "frmt": {
              "expression": ["xx.xx"],
              "missing": {},
              "scientific": {},
              "transform": {}
            }
          },
          "sd": {
            "frmt": {
              "expression": ["xx.xx"],
              "missing": {},
              "scientific": {},
              "transform": {}
            }
          }
        },
        "missing": {}
      }
    },
    {
      "group_val": [".default"],
      "label_val": [".default"],
      "param_val": ["median", "p25", "p75"],
      "frmt_combine": {
        "expression": ["{median} ({p25}, {p75})"],
        "frmt_ls": {
          "median": {
            "frmt": {
              "expression": ["xx.x"],
              "missing": {},
              "scientific": {},
              "transform": {}
            }
          },
          "p25": {
            "frmt": {
              "expression": ["xx.x"],
              "missing": {},
              "scientific": {},
              "transform": {}
            }
          },
          "p75": {
            "frmt": {
              "expression": ["xx.x"],
              "missing": {},
              "scientific": {},
              "transform": {}
            }
          }
        },
        "missing": {}
      }
    }
  ],
  "col_plan": {
    "col_plan": {
      "dots": [
        ["-context"],
        ["-starts_with(\"ord\")"]
      ],
      ".drop": [false]
    }
  },
  "sorting_cols": ["ord1", "ord2"],
  "big_n": {
    "param_val": ["bigN"],
    "n_frmt": {
      "expression": ["\nN = xx"],
      "missing": {},
      "scientific": {},
      "transform": {}
    },
    "by_page": [false]
  },
  "footnote_plan": {
    "struct_list": [
      {
        "column_val": ["Placebo"],
        "group_val": {},
        "label_val": {},
        "footnote_text": ["A footnote for Placebo"]
      }
    ],
    "marks": ["numbers"]
  }
}
